{
  "name": "@leiteszeke/base-graphql-prisma-mysql",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "generate": "yarn prisma:generate && graphql-codegen --config codegen.yml && copyfiles -u 1 src/**/*.graphql dist",
    "start": "node dist/index.js",
    "dev": "NODE_ENV=local ts-node-dev --respawn --transpile-only --exit-child --watch src src/index.ts",
    "dev:prod": "NODE_ENV=local ts-node --transpile-only src src/index.ts",
    "prisma:generate": "yarn prisma generate",
    "task": "NODE_ENV=local ts-node ./src/tasks/command.ts",
    "tsc": "tsc",
    "test": "jest --verbose --coverage",
    "test-ci": "yarn prisma:generate && yarn tsc && yarn test",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "build": "yarn generate && yarn tsc"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-resolvers": "^4.0.0",
    "@types/express-fileupload": "^1.4.1",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.2.5",
    "@types/pino-multi-stream": "^5.1.3",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "copyfiles": "^2.4.1",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^8.0.0",
    "@graphql-tools/load": "^8.0.0",
    "@graphql-tools/schema": "^10.0.0",
    "@prisma/client": "^4.15.0",
    "apollo-server-express": "^3.12.0",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "graphql": "^16.6.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-tag": "^2.12.6",
    "graphql-type-json": "^0.3.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "pino": "^8.14.1",
    "pino-multi-stream": "^6.0.0",
    "pino-tee": "^0.3.0",
    "prisma": "^4.15.0",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.6.2",
    "uuid": "^9.0.0"
  }
}
